                            <Path Fill="#E81123" Stretch="Fill" x:Name="p_errorName"
                              HorizontalAlignment="Right" Grid.Column="2"  Visibility="Collapsed" 
                             VerticalAlignment="Center" Width="15"  Height="15" Margin="5,5,15,5"
                              Data="M12,2 C6.48,2 2,6.48 2,12 C2,17.52 6.48,22 12,22 C17.52,22 22,17.52 22,12 C22,6.48 17.52,2 12,2 Z M12,13 C11.45,13 11,12.55 11,12 L11,8 C11,7.45 11.45,7 12,7 C12.55,7 13,7.45 13,8 L13,12 C13,12.55 12.55,13 12,13 Z M13,17 L11,17 L11,15 L13,15 L13,17 Z">
                                <Path.ToolTip>
                                    <ToolTip x:Name="tt_errorName" Content="Name Not Validate"  Style="{StaticResource ToolTipError}"/>
                                </Path.ToolTip>
                            </Path>
                        </Grid>

                        <Grid Grid.Row="3" Margin="5,1" >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Button x:Name="btn_add" Grid.Row="0" Grid.Column="0"
                          Background="#4E4E4E" BorderBrush="{x:Null}"
                  Margin="1,1"    Content="Add" Click="Btn_add_Click" VerticalAlignment="Top" />
                            <Button x:Name="btn_update" Grid.Row="0" Grid.Column="1"
                        Background="#4E4E4E" BorderBrush="{x:Null}"
                   Margin="1,1"    Content="Update" Click="Btn_update_Click" VerticalAlignment="Top" />
                            <Button x:Name="btn_delete" Grid.Row="0" Grid.Column="2"
                        Background="#4E4E4E" BorderBrush="{x:Null}"
                   Margin="1,1"    Content="Delete" Click="Btn_delete_Click" />

                        </Grid>


                    </Grid>
                    <Grid Grid.Row="2" >
                        <Grid.RowDefinitions >
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="7.5"/>

                        </Grid.RowDefinitions>
                        <Grid >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Text="Values" Foreground="#737373"
                                 FontSize="10" VerticalAlignment="Center" x:Name="txt_values"
                                       HorizontalAlignment="Left" Margin="5,5,0,5"/>
                            <Separator Background="#999999"   Width="340"  Margin="5,0,5,0"
                                      Grid.Column="1" VerticalAlignment="Center" />

                        </Grid>
                        <DataGrid  Background="White" IsReadOnly="true" Grid.Row="1"
                       SelectionChanged="Dg_subProperty_SelectionChanged"
                       x:Name="dg_subProperty" Width="340" 
                        AutoGenerateColumns="False" 
                        ColumnWidth="SizeToHeader" >
                            <DataGrid.Columns>
                                <DataGridTextColumn MinWidth="150"  Header="Values"   
                                                        Binding="{Binding Path=propertyItemName}">
                                    <DataGridTextColumn.ElementStyle>
                                        <Style TargetType="{x:Type TextBlock}">
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding isActive}" Value="0">
                                                    <Setter Property="TextDecorations" Value="Strikethrough"/>
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </DataGridTextColumn.ElementStyle>
                                </DataGridTextColumn>
                            </DataGrid.Columns>

                        </DataGrid>
                        <Separator Background="#999999"   Width="340"  Margin="5"
                                      Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Center" />
                        <Grid  Grid.Row="3" >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto"/>
                                <RowDefinition Height="auto"/>
                            </Grid.RowDefinitions>
                            <Grid >
                                <TextBox x:Name="tb_valueName"  Text="{Binding propertyItemName, Mode=OneWay}" Padding="10,5,5,5" 
                                     BorderThickness="0,0,0,2" materialDesign:HintAssist.Hint="Value..."  
                                     Foreground="#4E4E4E"  TextBlock.FontSize="12" Background="#f8f8f8"
                                     BorderBrush="AliceBlue" MaxLength="20" 
                                     HorizontalAlignment="Left" VerticalAlignment="Center" 
                                     materialDesign:TextFieldAssist.CharacterCounterStyle="{Binding}"
                                     Height="35" Width="340"  Margin="10"  />

                                <Path Fill="#E81123" Stretch="Fill" x:Name="p_errorNameSub"
                              HorizontalAlignment="Right" Grid.Column="1"  Visibility="Collapsed" 
                             VerticalAlignment="Center" Width="15"  Height="15" Margin="5,5,15,5"
                              Data="M12,2 C6.48,2 2,6.48 2,12 C2,17.52 6.48,22 12,22 C17.52,22 22,17.52 22,12 C22,6.48 17.52,2 12,2 Z M12,13 C11.45,13 11,12.55 11,12 L11,8 C11,7.45 11.45,7 12,7 C12.55,7 13,7.45 13,8 L13,12 C13,12.55 12.55,13 12,13 Z M13,17 L11,17 L11,15 L13,15 L13,17 Z">
                                    <Path.ToolTip>
                                        <ToolTip x:Name="tt_errorNameSub" Content="Name Not Validate"  Style="{StaticResource ToolTipError}"/>
                                    </Path.ToolTip>
                                </Path>
                            </Grid>

                            <Separator Background="#999999"   Width="340"  Margin="5"
                                      Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Center" />

                        </Grid>
                        <Grid Grid.Row="4">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>

                            </Grid.ColumnDefinitions>
                            <Button x:Name="btn_addValue"  Grid.Column="0"
                          Background="#4E4E4E" BorderBrush="{x:Null}"  Height="Auto"
                    Margin="5,1"    Content="Add" Click="Btn_addValue_Click" />
                            <Button x:Name="btn_updateValue"  Grid.Column="1"
                          Background="#4E4E4E" BorderBrush="{x:Null}"  Height="33"
                    Margin="5,1,5,0"    Content="Update" Click="Btn_updateValue_Click" VerticalAlignment="Top" />
                            <Button x:Name="btn_deleteValue"  Grid.Column="2"
                          Background="#4E4E4E" BorderBrush="{x:Null}"  Height="33"
                    Margin="5,1"    Content="Delete" Click="Btn_deleteValue_Click" />

                        </Grid>

                    </Grid>

                </Grid>

            </Grid>
        </Border>
    </Grid>
</UserControl>
